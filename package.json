{
  "name": "fytepycker",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently --kill-others --raw \"PYTHONUNBUFFERED=1 python manage.py runserver\" \"npm run redis\" \"npm run daphne\" \"cd frontend && npm run dev\"",
    "createsuperuser": "python manage.py createsuperuser",
    "makemigrations": "python manage.py makemigrations",
    "migrate": "python manage.py migrate",
    "server": "concurrently --kill-others --raw \"npm run build && PYTHONUNBUFFERED=1 python manage.py runserver\" \"npm run redis\" \"npm run daphne\"",
    "redis": "redis-server",
    "daphne": "daphne -b 127.0.0.1 -p 8001 backend.core.asgi:application",
    "test": "python manage.py test",
    "build": "./scripts/build.sh",
    "deploy": "./scripts/deploy.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dziermanfelix/fytepycker.git"
  },
