  "main": "index.js",
  "scripts": {
    "dev": "concurrently --kill-others --raw \"redis-server\" \"PYTHONUNBUFFERED=1 python manage.py runserver\" \"daphne -b 127.0.0.1 -p 8001 core.asgi:application\" \"cd frontend && npm run dev\" \"npm run celery\" \"npm run beat\"",
    "celery": "celery -A core worker --loglevel=info",
    "beat": "celery -A core beat --loglevel=info --scheduler django_celery_beat.schedulers:DatabaseScheduler",
    "test": "python manage.py test"
  },
